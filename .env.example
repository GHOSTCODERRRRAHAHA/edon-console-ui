# EDON Agent UI Environment Variables
# Copy this file to .env and update values

# EDON Gateway URL
VITE_EDON_GATEWAY_URL=http://localhost:8000

# API Token (optional - set for integration tests; use same value as EDON_API_TOKEN in gateway .env)
# Can also be set in the UI Settings page
VITE_EDON_API_TOKEN=

# Mock Mode (false = production mode, true = development mock data)
VITE_EDON_MOCK_MODE=false
